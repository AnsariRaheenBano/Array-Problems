#include <iostream>

using namespace std;

int main()
{
    
    int n,i;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int start = 0;
    int end = n - 1;
    while(start<end)
    {
        int temp;
        temp = a[start];
        a[start]=a[end];
        a[end]=temp;
        start++;
        end--;
    }
    for(i=0;i<n;i++)
    {
        cout<<a[i]<<endl;
    }
    
    return 0;
}
